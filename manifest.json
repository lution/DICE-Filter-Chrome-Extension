{
  "manifest_version": 3,
  "name": "Dice.com Job Search Resule Filter",
  "version": "1.0",
  "action": {
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.dice.com/job*",
    "https://dice.com/job*"
  ],
  "content_scripts": [
    {
        "js" : ["js/content.js"],
        "matches" : [
            "https://www.dice.com/job*",
            "https://dice.com/job*"
        ]
    }
  ]
}

